<%= render "layouts/errors", :object => @review %>
<div class="jumbotron">
  <h2>Edit Review by <%= @review.author %></h2>
</div>
<div class="content">
  <%=form_for [@product, @review] do |f|%>
  <%=f.label :Review_Author%>
  <%=f.text_field :author%>
  <%=f.label :Rating%>
  <%= f.number_field :rating, min: 1, max: 5, step: 1 %>
  <%=f.label :Review_Body%>
  <%=f.text_area :content_body%>
  <%=f.submit%>
  <%end%>
</div>