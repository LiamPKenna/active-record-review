<div class="content in-out-new">
  <br>
  <h1> Sign in</h1>
  <br>
  <%= form_tag signin_path do %>
    <table class="table user">
      <tr>
        <td><%= label_tag "Email" %></td>
        <td><%= text_field_tag :email %></td>
      </tr>
      <tr>
        <td><%= label_tag "Password" %></td>
        <td><%= password_field_tag :password %></td>
      </tr>
    </table>
    <%= submit_tag "Sign In" %>
  <% end %>
</div>